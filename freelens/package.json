    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsonfile": "^6.1.0",
    "jsonpath-plus": "^10.3.0",
    "mini-css-extract-plugin": "^2.9.2",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "node-abi": "^4.2.0",
    "node-loader": "^2.1.0",
