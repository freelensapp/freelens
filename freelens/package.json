    "@types/webpack": "^5.28.5",
    "circular-dependency-plugin": "^5.2.2",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^6.11.0",
    "electron": "^35.1.4",
    "electron-builder": "^26.0.6",
    "esbuild-loader": "^4.3.0",
