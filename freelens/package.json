{
  "name": "freelens",
  "private": true,
  "productName": "Freelens",
  "description": "Freelens - Free IDE for Kubernetes",
  "homepage": "https://freelens.app",
  "version": "1.2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freelensapp/freelens.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/freelensapp/freelens/issues"
  },
  "main": "static/build/main.js",
  "copyright": "Â© 2024-2025 Freelens Authors",
  "license": "MIT",
  "author": {
    "name": "Freelens Authors",
    "email": "freelens@freelens.app"
  },
  "scripts": {
    "clean": "pnpm dlx rimraf binaries dist static/build",
    "clean:node_modules": "pnpm dlx rimraf node_modules",
    "build": "pnpm dlx cross-env NODE_ENV=production webpack --config webpack/webpack.ts --progress",
    "postbuild": "pnpm \"/^postbuild:/\"",
    "postbuild:generate-tray-icons": "generate-tray-icons --output static/build/tray --input @freelensapp/icon/icons/logo-lens.svg --notice-icon @freelensapp/icon/icons/notice.svg --spinner-icon @freelensapp/icon/icons/arrow-spinner.svg",
    "postbuild:ensure-binaries": "ensure-binaries --package package.json --base-dir binaries/client",
    "build:app": "run-script-os --",
    "build:app:darwin": "electron-builder --publish never --macos",
    "build:app:default": "electron-builder --publish never",
    "build:app:linux": "electron-builder --publish never --linux",
    "build:app:win32": "electron-builder --publish never --win",
    "electron-rebuild": "electron-rebuild -f",
    "start": "run-script-os --",
    "start:darwin": "./dist/mac*/Freelens.app/Contents/MacOS/Freelens",
    "start:default": "exit 1",
    "start:linux": "./dist/linux*-unpacked/freelens",
    "start:win32": "dist\\win-unpacked\\Freelens.exe",
    "dev": "concurrently -i -k \"pnpm dev:run -C\" npm:dev:build:*",
    "dev:run": "nodemon --watch ./static/build/main.js --exec \"electron --remote-debugging-port=9223 --inspect .\"",
    "dev:build:main": "pnpm dlx cross-env NODE_ENV=development webpack --config webpack/main.ts --progress --watch",
    "dev:build:renderer": "pnpm dlx cross-env NODE_ENV=development ts-node ./webpack/dev-server.ts",
    "test:integration": "jest -xyz --runInBand --detectOpenHandles --modulePaths=[\"<rootDir>/integration/\"]"
  },
  "config": {
    "k8sProxyVersion": "1.2.8",
    "bundledKubectlVersion": "1.32.3",
    "bundledHelmVersion": "3.17.2",
    "contentSecurityPolicy": "script-src 'unsafe-eval' 'self'; frame-src https://*.renderer.freelens.app:*/; img-src * data:",
    "welcomeRoute": "/welcome"
  },
  "engines": {
    "node": "^22.0.0"
  },
  "dependencies": {
    "@freelensapp/animate": "workspace:^",
    "@freelensapp/application": "workspace:^",
    "@freelensapp/application-for-electron-main": "workspace:^",
    "@freelensapp/button": "workspace:^",
    "@freelensapp/cluster-settings": "workspace:^",
    "@freelensapp/cluster-sidebar": "workspace:^",
    "@freelensapp/core": "workspace:^",
    "@freelensapp/ensure-binaries": "workspace:^",
    "@freelensapp/error-boundary": "workspace:^",
    "@freelensapp/event-emitter": "workspace:^",
    "@freelensapp/feature-core": "workspace:^",
    "@freelensapp/icon": "workspace:^",
    "@freelensapp/json-api": "workspace:^",
    "@freelensapp/keyboard-shortcuts": "workspace:^",
    "@freelensapp/kube-api": "workspace:^",
    "@freelensapp/kube-api-specifics": "workspace:^",
    "@freelensapp/kube-object": "workspace:^",
    "@freelensapp/kubectl-versions": "workspace:^",
    "@freelensapp/kubernetes-client-node": "workspace:^",
    "@freelensapp/legacy-extensions": "workspace:^",
    "@freelensapp/legacy-global-di": "workspace:^",
    "@freelensapp/list-layout": "workspace:^",
    "@freelensapp/logger": "workspace:^",
    "@freelensapp/messaging": "workspace:^",
    "@freelensapp/messaging-for-main": "workspace:^",
    "@freelensapp/messaging-for-renderer": "workspace:^",
    "@freelensapp/metrics": "workspace:^",
    "@freelensapp/node-fetch": "workspace:^",
    "@freelensapp/notifications": "workspace:^",
    "@freelensapp/prometheus": "workspace:^",
    "@freelensapp/random": "workspace:^",
    "@freelensapp/react-application": "workspace:^",
    "@freelensapp/resizing-anchor": "workspace:^",
    "@freelensapp/resource-templates": "workspace:^",
    "@freelensapp/routing": "workspace:^",
    "@freelensapp/run-many": "workspace:^",
    "@freelensapp/spinner": "workspace:^",
    "@freelensapp/startable-stoppable": "workspace:^",
    "@freelensapp/tooltip": "workspace:^",
    "@freelensapp/utilities": "workspace:^",
    "@ogre-tools/injectable": "^17.11.1",
    "@ogre-tools/injectable-extension-for-auto-registration": "^17.2.0",
    "@ogre-tools/injectable-extension-for-mobx": "^17.2.0",
