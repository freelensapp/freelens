  "scripts": {
    "build": "pnpm dlx cross-env NODE_ENV=production webpack --config webpack/library-bundle.ts --progress",
    "clean": "pnpm dlx rimraf dist static/build",
    "clean:node_modules": "pnpm dlx rimraf node_modules",
    "test:unit": "jest",
    "test:watch": "func() { jest ${1} --watch --testPathIgnorePatterns integration; }; func",
    "lint": "pnpm dlx cross-env PROD=true eslint --ext js,ts,tsx --max-warnings=0 .",
    "lint:fix": "pnpm lint --fix"
  },
