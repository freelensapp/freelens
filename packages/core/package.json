  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack/library-bundle.ts --progress && linkable-push",
    "clean": "rimraf dist static/build",
    "test:unit": "jest --testPathIgnorePatterns integration",
    "test:watch": "func() { jest ${1} --watch --testPathIgnorePatterns integration; }; func",
    "lint": "PROD=true eslint --ext js,ts,tsx --max-warnings=0 .",
    "lint:fix": "npm run lint -- --fix"
  },
  "config": {
    "k8sProxyVersion": "1.2.2",
    "bundledKubectlVersion": "1.32.1",
    "bundledHelmVersion": "3.17.1",
    "sentryDsn": "",
    "contentSecurityPolicy": "script-src 'unsafe-eval' 'self'; frame-src https://*.lens.app:*/; img-src * data:",
