{
  "name": "@freelensapp/generate-tray-icons",
  "version": "1.0.0",
  "description": "CLI generating tray icons for building a lens-like application",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "swc ./src/index.ts -d ./dist/",
    "generate": "node --experimental-import-meta-resolve ./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.53",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^16.18.25",
    "@types/sharp": "^0.31.1"
  },
  "dependencies": {
    "arg": "^5.0.2",
    "jsdom": "^21.1.0",
    "rimraf": "^4.4.1",
    "sharp": "^0.32.1"
  }
}
