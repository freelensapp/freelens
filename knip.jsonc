    "packages/ensure-binaries": {
      "entry": ["bin/ensure-binaries.js!", "dist/index.mjs!", "src/index.mts"],
      "project": ["bin/*.js!", "dist/*.mjs!", "src/*.mts"],
      "typescript": {
        "config": ["tsconfig.json"]
      }
    },

    "packages/extensions": {
      "entry": [
        "dist/extension-api.js!",
        "index.ts",
        "jest.config.js",
        "src/**/*.injectable.ts",
        "webpack/extensions.ts"
      ],
      "project": [
        "*.js",
        "*.ts",
        "__mocks__/**/*.{ts,tsx}",
        "dist/**/*.js!",
        "dist/**/*.ts",
        "src/**/*.ts",
        "webpack/**/*.ts"
