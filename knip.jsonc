      "typescript": {
        "config": ["tsconfig.json"]
      },
      "webpack": {
        "config": ["webpack/extensions.ts"]
      }
    },

    "packages/generate-tray-icons": {
      "entry": ["bin/generate-tray-icons.js!", "dist/index.js!"],
      "project": ["bin/*.js!", "dist/*.js!", "src/*.ts"],
